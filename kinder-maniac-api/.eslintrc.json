{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "semi": [
      "error",
      "never"
    ],
    "consistent-return": "off",
    "object-curly-newline": "off",
    "linebreak-style": "off"
  }
}
